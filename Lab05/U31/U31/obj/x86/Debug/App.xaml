<Application
    x:Class="U31.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:U31">
    <Application.Resources>
        <Style TargetType="Image" x:Key="WhitelogoStyle">
            <Setter Property="Source" Value="https://i.pinimg.com/564x/0d/b3/bf/0db3bf073fd3e0b4ff0dbf9d92d68b8d.jpg"></Setter>
            <Setter Property="Width" Value="150"></Setter>
            <Setter Property="Margin" Value="20,20,20,0"></Setter>
            <Setter Property="HorizontalAlignment" Value="Left"></Setter>
        </Style>
    </Application.Resources>
</Application>

